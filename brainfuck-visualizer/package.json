{
  "name": "brainfuck-visualizer",
  "version": "1.0.0",
  "description": "A brainfuck visualizer built as a companion to the brain programming language compiler",
  "main": "./app/main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "dev": "./node_modules/.bin/webpack-dev-server --content-base dist/ --inline --hot --port 8080",
    "build": "./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint '**/*.js' '**/*.jsx'; exit 0",
    "test": "./node_modules/.bin/mocha --reporter spec --compilers jsx:babel-register \"tests/**/*.tests.js\" \"tests/**/*.tests.jsx\"",
    "clean": "rm -rf dist/"
  },
  "repository": "https://github.com/sunjay/brain",
  "keywords": [
    "brainfuck",
    "visualizer",
    "brain",
    "interpreter"
  ],
  "author": "Sunjay Varma",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "electron": "^1.4.1",
    "chai": "^3.5.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.9.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "redux-devtools": "^3.3.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-fontawesome": "^1.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  }
